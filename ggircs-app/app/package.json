    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-plugin-module-resolver": "^4.1.0",
