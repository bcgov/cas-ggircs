{
  "name": "ggircs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn build:schema && yarn build:relay",
    "dev": "node server",
    "build:schema": "postgraphile -X -c postgres://localhost/ggircs -s ggircs_app --export-schema-graphql server/schema.graphql --export-schema-json server/schema.json --classic-ids --sort-export && echo >> server/schema.json",
    "build:relay": "relay-compiler --verbose",
    "build:next": "next build",
    "build": "yarn build:schema && yarn build:relay && yarn build:next",
    "start": "NODE_ENV=production node --unhandled-rejections=strict server",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "pretest": "npm run build:relay",
    "test": "jest",
    "test:e2e-snapshots": "yarn happo-cypress -- -- cypress run",
    "test:e2e": "cypress run",
    "cypress": "cypress open"
  },
  "dependencies": {
    "@button-inc/bcgov-theme": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@graphile-contrib/pg-many-to-many": "^1.0.0",
    "body-parser": "^1.19.0",
    "bowser": "^2.11.0",
    "connect-pg-simple": "^6.2.1",
    "consola": "^2.15.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphile-build": "^4.10.0",
    "graphile-build-pg": "^4.10.0",
    "graphql": "^15.4.0",
    "improved-yarn-audit": "^2.3.3",
    "isomorphic-fetch": "^3.0.0",
    "keycloak-connect": "^12.0.1",
    "morgan": "^1.10.0",
    "next": "10.2.3",
    "pg": "^8.5.1",
    "postgraphile": "^4.12.3",
    "postgraphile-log-consola": "^1.0.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^16.14.0",
    "react-relay": "^10.1.2",
    "react-relay-network-modern": "^6.0.0",
    "react-toastify": "^6.2.0",
    "relay-runtime": "^10.1.2",
    "styled-jsx": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-relay": "^7.0.17",
    "@types/relay-runtime": "^10.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-relay": "^10.1.2",
    "cypress": "^6.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-relay": "^1.8.1",
    "graphql-query-test-mock": "^0.12.1",
    "happo-cypress": "^1.16.1",
    "happo.io": "^6.4.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "jest-extended": "^0.11.5",
    "jest-raw-loader": "^1.0.1",
    "nock": "^13.0.7",
    "prettier": "^2.2.1",
    "relay-compiler": "^10.1.2",
    "relay-compiler-language-typescript": "^13.0.2",
    "relay-config": "^10.1.2",
    "typescript": "^4.1.3",
    "wait-on": "^5.2.1"
  },
  "resolutions": {
    "**/hosted-git-info": "4.0.1"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/cypress/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less)$": "identity-obj-proxy",
      "react-syntax-highlighter/dist/esm/(.*)": "react-syntax-highlighter/dist/cjs/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>tests/setupTests.ts",
      "jest-canvas-mock",
      "jest-extended"
    ],
    "transform": {
      ".*(ts|tsx|js|jsx)$": "babel-jest",
      "\\.graphql$": "jest-raw-loader"
    }
  }
}
