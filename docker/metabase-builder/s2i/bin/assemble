#!/bin/bash

# restore build artifacts
if [ "$(ls /tmp/artifacts/ 2>/dev/null)" ]; then
    mv /tmp/artifacts/* $HOME/.
fi

# move the application source
mv /tmp/src $HOME/src

# build application artifacts
pushd ${HOME}/src
pwd
ls -al .
./bin/build
popd

mv ${HOME}/src/target/uberjar/metabase.jar $HOME
